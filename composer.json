{
    "name": "danabrey/supernova-api",
    "description": "PHP API wrapper for supernovafantasyfootball.com",
    "type": "library",
    "license": "MIT",
    "version": "v0.1.1",
    "authors": [
        {
            "name": "Dan Abrey",
            "email": "danabrey@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DanAbrey\\SupernovaApi\\": "src/",
            "DanAbrey\\SupernovaApi\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "symfony/serializer": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/property-access": "^5.2",
        "ext-json": "*",
        "spatie/data-transfer-object": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --colors=always tests"
